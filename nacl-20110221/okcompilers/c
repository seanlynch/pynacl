gcc -m64 -O3 -fomit-frame-pointer -funroll-loops -fPIC
gcc -m64 -O -fomit-frame-pointer -fPIC
gcc -m32 -O3 -fomit-frame-pointer -funroll-loops -fPIC
gcc -m32 -O -fomit-frame-pointer -fPIC
spu-gcc -mstdmain -march=cell -O3 -funroll-loops -fomit-frame-pointer -Drandom=rand -Dsrandom=srand -fPIC
spu-gcc -mstdmain -march=cell -O -fomit-frame-pointer -Drandom=rand -Dsrandom=srand -fPIC
